{
    "flex-layout.row#footer-2-desktop": {
        "children": [
            "flex-layout.col#footer-2-col1",
            "flex-layout.col#footer-2-col2",
            "flex-layout.col#footer-2-col3",
            "flex-layout.col#footer-2-col4"
        ],
        "props": {
            "blockClass": "payment-methods",
            "paddingTop": 8,
            "paddingBottom": 8
        }
    }, 
    "flex-layout.col#footer-2-col1": {
        "children": [
            "rich-text#footer-2-col1-title",
            "vtex.menu@2.x:menu#footer_menu-1"
        ],
        "props": {
            "preventVerticalStretch": true
        }
    }, 
    "flex-layout.col#footer-2-col2": {
        "children": [
            "rich-text#footer-2-col2-title",
            "vtex.menu@2.x:menu#footer_menu-2"
        ],
        "props": {
            "preventVerticalStretch": true
        }
    }, 
    "flex-layout.col#footer-2-col3": {
        "children": [
            "rich-text#footer-2-col3-title",
            "vtex.menu@2.x:menu#footer_menu-3"
        ],
        "props": {
            "preventVerticalStretch": true
        }
    }, 
    "flex-layout.col#footer-2-col4": {
        "children": [
            "rich-text#footer-2-col4-title",
            "flex-layout.row#footer-redes-sociais"
        ],
        "props": {
            "preventVerticalStretch": true
        }
    }, 
    
    "flex-layout.row#footer-redes-sociais": {
        "children": [ 
            "image#footer-redes-sociais-1",
            "image#footer-redes-sociais-2",
            "image#footer-redes-sociais-3",
            "image#footer-redes-sociais-4",
            "image#footer-redes-sociais-5"
        ],
        "props": {
            "preventHorizontalStretch": true,
            "colGap": 7
        } 
    },
    // CONTEÚDOS
    "rich-text#footer-2-col1-title": {
        "props": {
            "text": "INSTITUCIONAL",
            "blockClass": "footer-title"
        }
    },
    
    "rich-text#footer-2-col2-title": {
        "props": {
            "text": "SUPORTE",
            "blockClass": "footer-title"
        }
    }, 
    
    "rich-text#footer-2-col3-title": {
        "props": {
            "text": "CONTATO",
            "blockClass": "footer-title"
        }
    },

    "rich-text#footer-2-col4-title": {
        "props": {
            "text": "Siga a @patogeoficial",
            "blockClass": "footer-title"
        }
    },

    "vtex.menu@2.x:menu#footer_menu-1": {
        "props": {
            "orientation": "vertical",
            "blockClass": "footer_menu",
            "items": [
                { 
                    "type": "custom",
                    "highlight": false,
                    "itemProps": {
                        "type": "internal",
                        "href": "/institucional/a-marca-patoge",
                        "noFollow": false,
                        "tagTitle": "A Marca",
                        "text": "A Marca"
                    }
                },
                { 
                    "type": "custom",
                    "highlight": false,
                    "itemProps": {
                        "type": "internal",
                        "href": "/onde-encontrar",
                        "noFollow": false,
                        "tagTitle": "Onde Encontrar",
                        "text": "Onde Encontrar"
                    }
                },
                { 
                    "type": "custom",
                    "highlight": false,
                    "itemProps": {
                        "type": "external",
                        "href": "https://patoge.com.br/blog/",
                        "noFollow": false,
                        "tagTitle": "Blog",
                        "text": "Blog"
                    }
                }
            ]
        }
    },

    "vtex.menu@2.x:menu#footer_menu-2": {
        "props": {
            "orientation": "vertical",
            "blockClass": "footer_menu",
            "items": [
                { 
