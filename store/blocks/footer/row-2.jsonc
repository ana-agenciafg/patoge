    "vtex.menu@2.x:menu#footer_menu-3": {
        "props": {
            "orientation": "vertical",
            "blockClass": "footer_menu",
            "items": [
                { 
                    "type": "custom",
                    "highlight": false,
                    "itemProps": {
                        "type": "internal",
                        "href": "/institucional/fale-conosco",
                        "noFollow": false,
                        "tagTitle": "Fale Conosco",
